{
    "name": "siganushka/api-client",
    "description": "API Client.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "siganushka/registry-contracts": "^0.1",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/options-resolver": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\ApiClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
